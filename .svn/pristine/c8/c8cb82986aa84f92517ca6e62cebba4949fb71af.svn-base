/*
 * Skin: skin-green-light
 * ----------
 */
.skin-green-light .main-header .navbar {
  background-color: #26b373;
}
.skin-green-light .main-header .navbar .nav > li > a {
  color: #fff;
}
.skin-green-light .main-header .navbar .nav > li > a:hover,
.skin-green-light .main-header .navbar .nav > li > a:active,
.skin-green-light .main-header .navbar .nav > li > a:focus,
.skin-green-light .main-header .navbar .nav .open > a,
.skin-green-light .main-header .navbar .nav .open > a:hover,
.skin-green-light .main-header .navbar .nav .open > a:focus,
.skin-green-light .main-header .navbar .nav > .active > a {
  background: rgba(0, 0, 0, 0.1);
  color: #fff;
}
.skin-green-light .main-header .navbar .sidebar-toggle {
  color: #fff;
}
.skin-green-light .main-header .navbar .sidebar-toggle,
.skin-green-light .main-header .navbar .sidebar-toggle:hover {
  color: #fff;
}
.skin-green-light .main-header .navbar .refresh_iframe,
.skin-green-light .main-header .navbar .refresh_iframe:hover {
  color: #fff;
}
.skin-green-light .main-header .logo {
  background-color: #008d4c !important;
  color: #ffffff;
  border-bottom: 0 solid transparent;
}
.skin-green-light .main-header .logo:hover {
  background-color: #008d4c !important;
}
.skin-green-light .main-header li.user-header {
  background-color: #344058;
}
.skin-green-light .content-header {
  background: transparent;
}

.skin-green-light .main-sidebar,
.skin-green-light .left-side {
  background-color: #fff !important;
  border-right: 1px solid #E6E6E6;
}
.skin-green-light .user-panel > .info,
.skin-green-light .user-panel > .info > a {
  color: #fff;
}
.skin-green-light .sidebar-menu > li.header {
  color: #4b646f;
  background: #1a2226;
}
.skin-green-light .sidebar-menu > li > a {
  border-left: 4px solid transparent;
  color: #333;
  height: 60px;
  line-height: 60px;
  padding: 0px 20px;
}
.skin-green-light .sidebar-menu > li > a:hover{
  background: rgba(255, 255, 255, .03);
  border-left-color: #26b373;
}

.skin-green-light .sidebar-menu > li:hover > a,
.skin-green-light .sidebar-menu > li.active > a,
.skin-green-light .sidebar-menu > li.menu-open > a {
  color: #333;
}
.skin-green-light .sidebar-menu > li.active > a {
  border-left-color: #26b373;
}

.skin-green-light .treeview-menu>li>a{
  padding: 0;
}

.skin-green-light .treeview > .treeview-menu{
  background-color: #F2F2F2 !important;
  padding-left: 0px;
}

.skin-green-light.sidebar-mini.sidebar-collapse .treeview > .treeview-menu{
  background: #ffffff !important;
  box-shadow: 0 2px 4px rgba(0, 0, 0, .12);
  border: 1px solid #dddddd;
}

.skin-green-light.sidebar-mini.sidebar-collapse .treeview > .treeview-menu > .treeview > .treeview-menu{
  background-color: #F2F2F2 !important;
  padding-left: 0px;
  box-shadow: none;
  border: 0;
}

.skin-green-light .sidebar a {
  color: #b8c7ce;
}
.skin-green-light .sidebar a:hover {
  text-decoration: none;
}
.skin-green-light .sidebar-menu .treeview-menu > li > a {
  color: #333;
  height: 40px;
  line-height: 40px;
}
.skin-green-light .sidebar-menu .treeview-menu > li.active > a{
  color: #ffffff;
}
.skin-green-light .sidebar-form {
  border-radius: 3px;
  border: 1px solid #374850;
  margin: 10px 10px;
}
.skin-green-light .sidebar-form input[type="text"],
.skin-green-light .sidebar-form .btn {
  box-shadow: none;
  background-color: #374850;
  border: 1px solid transparent;
  height: 35px;
}
.skin-green-light .sidebar-form input[type="text"] {
  color: #666;
  border-top-left-radius: 2px;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
  border-bottom-left-radius: 2px;
}
.skin-green-light .sidebar-form input[type="text"]:focus,
.skin-green-light .sidebar-form input[type="text"]:focus + .input-group-btn .btn {
  background-color: #fff;
  color: #666;
}
.skin-green-light .sidebar-form input[type="text"]:focus + .input-group-btn .btn {
  border-left-color: #fff;
}
.skin-green-light .sidebar-form .btn {
  color: #999;
  border-top-left-radius: 0;
  border-top-right-radius: 2px;
  border-bottom-right-radius: 2px;
  border-bottom-left-radius: 0;
}
.skin-green-light.layout-top-nav .main-header > .logo {
  background-color: #002140;
  color: #ffffff;
  border-bottom: 0 solid transparent;
}
.skin-green-light.layout-top-nav .main-header > .logo:hover {
  background-color: #002140;
}

.skin-green-light .page-tabs a.active:after{
  background-color: #26b373 !important;;
}

.skin-green-light .treeview-menu>li.active {
  background: #26b373;
}

.skin-green-light .page-tabs a,
.skin-green-light .page-tabs a:hover
.skin-green-light .page-tabs a:focus
.skin-green-light .page-tabs a:visited{
  color: #666;
}

.skin-green-light .page-tabs a.active {
  color: #333;
}

.skin-green-light .page-tabs a.active:after{
  background-color: #26b373;
  top: 38px;
}
.skin-green-light .main-footer a,
.skin-green-light .main-footer a:hover,
.skin-green-light .main-footer a:focus,
.skin-green-light .main-footer a:visited{
  color: #26b373;
}


