/*
 * Skin: gray-light
 * ----------
 */
.skin-gray-pro .main-header .navbar {
    background: #fff;
    box-shadow: 0 1px 0px 0 rgba(0, 0, 0, .05);
}
.skin-gray-pro .main-header .navbar .nav > li > a {
    color: #666;
}
.skin-gray-pro .main-header .navbar .nav > li > a:hover,
.skin-gray-pro .main-header .navbar .nav > li > a:active,
.skin-gray-pro .main-header .navbar .nav > li > a:focus,
.skin-gray-pro .main-header .navbar .nav .open > a,
.skin-gray-pro .main-header .navbar .nav .open > a:hover,
.skin-gray-pro .main-header .navbar .nav .open > a:focus,
.skin-gray-pro .main-header .navbar .nav > .active > a {
    background: rgba(0, 0, 0, 0.1);
    color: #666;
}
.skin-gray-pro .main-header .navbar .sidebar-toggle {
    color: #ffffff;
}
.skin-gray-pro .main-header .navbar .sidebar-toggle,
.skin-gray-pro .main-header .navbar .sidebar-toggle:hover {
    color: #666;
}
.skin-gray-pro .main-header .navbar .refresh_iframe,
.skin-gray-pro .main-header .navbar .refresh_iframe:hover {
    color: #666;
}
.skin-gray-pro .main-header .logo {
    background-color: #434652;
    color: #e6e6e6;
    border-bottom: 0 solid transparent;
}
.skin-gray-pro .main-header .logo:hover {
    background-color: #3A3D49;
}
.skin-gray-pro .main-header li.user-header {
    background-color: #3A3D49;
}
.skin-gray-pro .content-header {
    background: transparent;
}

.skin-gray-pro .main-sidebar,
.skin-gray-pro .left-side {
    background-color: #3A3D49 !important
}
.skin-gray-pro .user-panel > .info,
.skin-gray-pro .user-panel > .info > a {
    color: #fff;
}
.skin-gray-pro .sidebar-menu > li.header {
    color: #4b646f;
    background: #1a2226;
}
.skin-gray-pro .sidebar-menu > li > a {
    border-left: 4px solid transparent;
    height: 60px;
    line-height: 60px;
    padding: 0px 20px;
}
.skin-gray-pro .sidebar-menu > li > a:hover{
    background: rgba(255, 255, 255, .03);
    border-left-color: #1e9fff;
}

.skin-gray-pro .sidebar-menu > li:hover > a,
.skin-gray-pro .sidebar-menu > li.active > a,
.skin-gray-pro .sidebar-menu > li.menu-open > a {
    color: #ffffff;
}
.skin-gray-pro .sidebar-menu > li.active > a {
    border-left-color: #1e9fff;
}

.skin-gray-pro .treeview-menu>li>a{
    padding: 0;
}

.skin-gray-pro .treeview > .treeview-menu{
    background-color: rgba(0, 0, 0, .3) !important;
    padding-left: 0px;
}

.skin-gray-pro.sidebar-mini.sidebar-collapse .treeview > .treeview-menu{
    background-color: #3A3D49 !important;
    padding-left: 0px;
}

.skin-gray-pro.sidebar-mini.sidebar-collapse .treeview > .treeview-menu > .treeview > .treeview-menu{
    background-color: rgba(0, 0, 0, .3) !important;
    padding-left: 0px;
}

.skin-gray-pro .sidebar a {
    color: rgba(255,255,255,.7);
}
.skin-gray-pro .sidebar a:hover {
    text-decoration: none;
}
.skin-gray-pro .sidebar-menu .treeview-menu > li > a {
    height: 40px;
    line-height: 40px;
}

.skin-gray-pro .sidebar-menu .treeview-menu > li:hover > a,
.skin-gray-pro .sidebar-menu .treeview-menu > li.active > a,
.skin-gray-pro .sidebar-menu .treeview-menu > li.menu-open > a{
    color: #fff;
}

.skin-gray-pro .sidebar-form {
    border-radius: 3px;
    border: 1px solid #374850;
    margin: 10px 10px;
}
.skin-gray-pro .sidebar-form input[type="text"],
.skin-gray-pro .sidebar-form .btn {
    box-shadow: none;
    background-color: #374850;
    border: 1px solid transparent;
    height: 35px;
}
.skin-gray-pro .sidebar-form input[type="text"] {
    color: #666;
    border-top-left-radius: 2px;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 2px;
}
.skin-gray-pro .sidebar-form input[type="text"]:focus,
.skin-gray-pro .sidebar-form input[type="text"]:focus + .input-group-btn .btn {
    background-color: #fff;
    color: #666;
}
.skin-gray-pro .sidebar-form input[type="text"]:focus + .input-group-btn .btn {
    border-left-color: #fff;
}
.skin-gray-pro .sidebar-form .btn {
    color: #999;
    border-top-left-radius: 0;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    border-bottom-left-radius: 0;
}
.skin-gray-pro.layout-top-nav .main-header > .logo {
    background-color: #3A3D49;
    color: #ffffff;
    border-bottom: 0 solid transparent;
}
.skin-gray-pro.layout-top-nav .main-header > .logo:hover {
    background-color: #3A3D49;
}
.skin-gray-pro .page-tabs a,
.skin-gray-pro .page-tabs a:hover
.skin-gray-pro .page-tabs a:focus
.skin-gray-pro .page-tabs a:visited{
    color: #666;
}

.skin-gray-pro .page-tabs a.active {
    color: #000;
}
.skin-gray-pro .page-tabs a.active:after{
    background-color: #434652 !important;;
}

.skin-gray-pro .treeview-menu> li.active {
    background: #1e9fff;
}

.skin-gray-pro .main-footer a,
.skin-gray-pro .main-footer a:hover,
.skin-gray-pro .main-footer a:focus,
.skin-gray-pro .main-footer a:visited{
    color: #434652;
}

